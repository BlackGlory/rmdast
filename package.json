{
  "name": "rmdast",
  "version": "0.1.0",
  "description": "",
  "keywords": [],
  "files": [
    "lib",
    "dist"
  ],
  "main": "lib/es2018/index.js",
  "types": "lib/es2018/index.d.ts",
  "module": "dist/es2018/index.mjs",
  "browser": "dist/es2018/index.umd.js",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "jest --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "coverage": "jest --coverage --config jest.config.js",
    "prepublishOnly": "yarn clean && yarn build",
    "clean": "rm -rf lib",
    "build": "yarn build:es2015 && yarn build:es2018",
    "build:es2015": "tsc --project tsconfig.build.json --target es2015 --outDir lib/es2015 && tscpaths -p tsconfig.build.json -s ./src -o ./lib/es2015",
    "build:es2018": "tsc --project tsconfig.build.json --target es2018 --outDir lib/es2018 && tscpaths -p tsconfig.build.json -s ./src -o ./lib/es2018"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/common-tags": "^1.8.0",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.9",
    "@types/mdast": "^3.0.3",
    "@types/node": "^14.0.27",
    "@types/object-hash": "^1.3.3",
    "@types/react": "^16.9.46",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "common-tags": "^1.8.0",
    "eslint": "^7.6.0",
    "husky": "^4.2.5",
    "jest": "^26.4.1",
    "jest-extended": "^0.11.5",
    "ts-jest": "^26.2.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "hyntax": "^1.1.6",
    "mdast-util-definitions": "^3.0.1",
    "remark-footnotes": "^1.0.0",
    "remark-parse": "^8.0.3",
    "ts-is-present": "^1.1.3",
    "unified": "^9.2.0",
    "unist-util-is": "^4.0.2",
    "unist-util-visit": "^2.0.3"
  }
}
