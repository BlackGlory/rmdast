{
  "name": "rmdast",
  "version": "7.0.5",
  "description": "Renderable Markdown Abstract Syntax Tree",
  "keywords": [],
  "files": [
    "lib"
  ],
  "type": "module",
  "main": "lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./utils/*": "./lib/rmdast-utils/*.js"
  },
  "sideEffects": false,
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=16"
  },
  "repository": "git@github.com:BlackGlory/rmdast.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "prepare": "ts-patch install -s",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --runInBand --no-cache --config jest.config.js",
    "test:debug": "cross-env NODE_OPTIONS=--experimental-vm-modules node --inspect-brk jest --runInBand --config jest.config.js",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage --config jest.config.js",
    "prepublishOnly": "run-s prepare clean build",
    "clean": "rimraf lib",
    "build": "tsc --project tsconfig.build.json --outDir lib",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prepare lint build test",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/jest-resolver": "^0.3.0",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/jest": "^29.2.6",
    "@types/lodash": "^4.14.191",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.32.0",
    "extra-filesystem": "^0.4.8",
    "extra-tags": "^0.3.1",
    "husky": "4",
    "jest": "^29.3.1",
    "jest-resolve": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.1.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.5",
    "ts-patch": "^2.1.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3",
    "typescript-transform-paths": "^3.4.6"
  },
  "dependencies": {
    "@blackglory/errors": "^2.4.3",
    "@types/unist": "^2.0.6",
    "extra-utils": "^5.0.1",
    "hotypes": "^0.5.1",
    "iterable-operator": "^4.0.0",
    "lodash": "^4.17.21",
    "mdast-util-directive": "^2.2.1",
    "mdast-util-footnote": "^1.1.0",
    "mdast-util-from-markdown": "^1.2.0",
    "mdast-util-gfm": "^2.0.1",
    "micromark-extension-directive": "^2.1.1",
    "micromark-extension-footnote": "^1.0.2",
    "micromark-extension-gfm": "^2.0.1",
    "nanoid": "^4.0.0"
  }
}
